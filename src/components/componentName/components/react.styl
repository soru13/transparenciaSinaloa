.react-container-grilla
    display grid
    grid-template-columns repeat(16, 1fr)
    grid-template-rows repeat(2,1fr)
    width 100%
    height: 80vh;
    align-self center
    justify-self center
    grid-column-gap: 1vh;
    grid-row-gap: 5vh;
    background: red;
    .item
        display grid
        align-items center
        justify-items center
        height 37vh
        grid-row-gap 1vh
        grid-column span 2
        img
            width 54px
            height 141px
            border-radius inherit!important;
