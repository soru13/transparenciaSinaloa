.react-container-grilla
    display grid
    background white
    justify-content center
    grid-template-rows repeat(4, 100%)
    overflow auto
    height 100vh
    .item
        display grid
        align-items end
        text-align center
        justify-items center
        width 100%
        height 100vh
    .item p a
        text-decoration underline
    #introduccion p
        align-self self-start
        width 80%
    .header_tabla
        display flex
        justify-content space-around
    .nivo
        height 600px
        position relative
        width 100%
        align-self center
    #tablaMontoProgramas, #educacionBasicaBenitoJuarez, #educacionSuperiorBenitoJuarez, #AdultosMayores, #jovenesconstruyendo, #jovenesEscribiendo
        align-items center
    .table
        height 80%
        overflow auto
    .item__header
        text-align center
        align-self self-end
    table
        text-align left
